{
  "name": "vue-vtable",
  "description": "Light weight vertical list component for Vue",
  "author": "Iwata Hidetaka <iwata0303@gmail.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "version": "0.11.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderful-panda/vue-vtable.git"
  },
  "keywords": [
    "Vue",
    "component",
    "table"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "tslint": "tslint -c tslint.json src/**/*.{ts,tsx} example/**/*.{ts,tsx}",
    "tslint:fix": "tslint -c tslint.json src/**/*.{ts,tsx} example/**/*.{ts,tsx} --fix",
    "prettier": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --list-different",
    "prettier:fix": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --write",
    "build-storybook": "build-storybook -c .storybook -o example",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-all": "npm-run-all tslint prettier build build-storybook"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "vue-css-definition": "^0.1.0",
    "vue-resizesensor": "^0.2.1",
    "vue-strict-prop": "^0.1.1"
  },
  "devDependencies": {
    "@storybook/vue": "^3.4.6",
    "@types/lodash": "4.14.109",
    "@types/node": "^8.0.34",
    "@types/storybook__vue": "^3.3.0",
    "@types/webpack-env": "^1.13.6",
    "babel-core": "^6.24.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.0",
    "html-loader": "^0.4.5",
    "npm-run-all": "^4.0.2",
    "prettier": "1.12.1",
    "storybook-addon-vue-info": "^0.4.2",
    "ts-loader": "^2.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.3",
    "vue": "^2.5.16",
    "vue-class-component": "^6.0.0",
    "vue-component-marker": "^0.1.0",
    "vue-loader": "14.2.1",
    "vue-template-compiler": "^2.5.16",
    "vue-template-compiler-loader": "^1.0.4",
    "vue-tsx-support": "^1.1.0",
    "vue-typed-component": "^0.10.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.2"
  },
  "peerDependencies": {
    "vue": "^2.5.2",
    "vue-class-component": "^6.0.0",
    "vue-tsx-support": "^1.1.0",
    "vue-typed-component": "^0.10.0"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 4
  }
}
