{
  "name": "vue-vtable",
  "description": "Light weight vertical list component for Vue",
  "author": "Iwata Hidetaka <iwata0303@gmail.com>",
  "main": "dist/vue-vtable.umd.js",
  "module": "dist/vue-vtable.esm.js",
  "typings": "dist/index.d.ts",
  "version": "1.0.0-beta.8a",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderful-panda/vue-vtable.git"
  },
  "keywords": [
    "Vue",
    "component",
    "table"
  ],
  "scripts": {
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/vue-vtable.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/vue-vtable.esm.js",
    "build": "npm-run-all build:umd build:es",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "prettier": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --list-different",
    "prettier:fix": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --write",
    "build-storybook": "build-storybook -c .storybook -o example",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-all": "npm-run-all lint prettier build build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/vue": "^5.2.1",
    "@types/lodash": "4.14.109",
    "@types/node": "^8.0.34",
    "@types/storybook__vue": "^5.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-loader": "^0.4.5",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.0.2",
    "prettier": "1.12.1",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-loader": "^6.1.0",
    "typescript": "^3.5.1",
    "vue": "^2.6.1",
    "vue-class-component": "^7.0.0",
    "vue-css-definition": "^0.1.0",
    "vue-loader": "15.7.1",
    "vue-property-decorator": "^8.2.2",
    "vue-resizesensor": "^0.2.1",
    "vue-strict-prop": "^0.1.1",
    "vue-template-compiler": "^2.6.1",
    "vue-tsx-support": "^3.0.0-dev.7a",
    "webpack": "^4.40.2"
  },
  "peerDependencies": {
    "vue": "^2.6.1",
    "vue-class-component": "^7.0.0",
    "vue-property-decorator": "^8.2.2",
    "vue-tsx-support": "^3.0.0-dev.3"
  }
}
