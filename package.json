{
  "name": "vue-vlist",
  "description": "Light weight vertical list component for Vue",
  "author": "Iwata Hidetaka <iwata0303@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "watch": "webpack -w",
    "watch-all": "webpack -w --config webpack.config.all.js",
    "tslint": "tslint --project tsconfig.json",
    "serve": "http-server -c 1 -a localhost",
    "dev": "npm-run-all --parallel watch-all serve"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "vue": "^1.0.26",
    "vueit": "^0.0.2",
    "vue-resizesensor": "^0.0.4"
  },
  "devDependencies": {
    "@types/lodash": "0.0.27",
    "@types/vue": "^1.0.26",
    "cross-env": "^2.0.0",
    "html-loader": "^0.4.3",
    "http-server": "^0.9.0",
    "npm-run-all": "^1.6.0",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.0",
    "webpack": "^1.13.2"
  }
}
