{
    "name": "vue-vtable",
    "description": "Light weight vertical list component for Vue",
    "author": "Iwata Hidetaka <iwata0303@gmail.com>",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "version": "1.0.1",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack",
        "lint": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --list-different",
        "lint:fix": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --write",
        "bundle-example": "webpack --config webpack.config.example.js",
        "build-all": "npm-run-all build bundle-example",
        "dev": "webpack-dev-server --config webpack.config.example.js"
    },
    "dependencies": {
        "lodash": "^4.17.4"
    },
    "devDependencies": {
        "@types/lodash": "4.14.85",
