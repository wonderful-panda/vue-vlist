{
  "name": "vue-vtable",
  "description": "Light weight vertical list component for Vue",
  "author": "Iwata Hidetaka <iwata0303@gmail.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "version": "0.8.2",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "tslint": "tslint -c tslint.json src/**/*.{ts,tsx} example/**/*.{ts,tsx}",
    "tslint:fix": "tslint -c tslint.json src/**/*.{ts,tsx} example/**/*.{ts,tsx} --fix",
    "prettier": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --list-different",
    "prettier:fix": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --write",
    "bundle-example": "webpack --config webpack.config.example.js",
    "build-all": "npm-run-all tslint prettier build bundle-example",
    "dev": "webpack-dev-server --config webpack.config.example.js"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "vue-css-definition": "0.1.0",
    "vue-resizesensor": "0.2.0",
    "vue-strict-prop": "^0.1.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.64",
    "@types/node": "^8.0.34",
    "babel-core": "^6.24.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.0",
    "html-loader": "^0.4.5",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.7.4",
    "pug": "^2.0.0-rc.1",
    "ts-loader": "^2.1.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.3.3",
    "vue": "^2.5.2",
    "vue-class-component": "^6.0.0",
    "vue-template-compiler": "^2.5.2",
    "vue-template-compiler-loader": "^1.0.4",
    "vue-tsx-support": "^1.1.0",
    "vue-typed-component": "^0.10.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "vue": "^2.5.2",
    "vue-class-component": "^6.0.0",
    "vue-tsx-support": "^1.1.0",
    "vue-typed-component": "^0.10.0"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 4
  }
}
