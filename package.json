{
  "name": "vue-vtable",
  "description": "Light weight vertical list component for Vue",
  "author": "Iwata Hidetaka <iwata0303@gmail.com>",
  "main": "dist/vue-vtable.umd.js",
  "module": "dist/vue-vtable.esm.js",
  "typings": "dist/index.d.ts",
  "version": "0.15.0-dev.1b",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderful-panda/vue-vtable.git"
  },
  "keywords": [
    "Vue",
    "component",
    "table"
  ],
  "scripts": {
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/vue-vtable.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/vue-vtable.esm.js",
    "build": "npm-run-all build:umd build:es",
    "tslint": "tslint -c tslint.json src/**/*.{ts,tsx} example/**/*.{ts,tsx}",
    "tslint:fix": "tslint -c tslint.json src/**/*.{ts,tsx} example/**/*.{ts,tsx} --fix",
    "prettier": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --list-different",
    "prettier:fix": "prettier \"{src,example/src}/**/*.{ts,tsx}\" --write",
    "build-storybook": "build-storybook -c .storybook -o example",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-all": "npm-run-all tslint prettier build build-storybook"
  },
  "dependencies": {
    "vue-css-definition": "^0.1.0",
    "vue-resizesensor": "^0.2.1",
    "vue-strict-prop": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@storybook/vue": "^3.4.6",
    "@types/lodash": "4.14.109",
    "@types/node": "^8.0.34",
    "@types/storybook__vue": "^3.3.0",
    "@types/webpack-env": "^1.13.6",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.0.0",
    "html-loader": "^0.4.5",
    "lodash": "^4.17.10",
    "npm-run-all": "^4.0.2",
    "prettier": "1.12.1",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "storybook-addon-vue-info": "^0.4.2",
    "ts-loader": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^3.5.1",
    "vue": "^2.6.1",
    "vue-class-component": "^7.0.0",
    "vue-component-marker": "^0.1.1",
    "vue-loader": "14.2.1",
    "vue-property-decorator": "^8.2.2",
    "vue-template-compiler": "^2.6.1",
    "vue-tsx-support": "^3.0.0-dev.6a",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-merge": "^4.2.2"
  },
  "peerDependencies": {
    "vue": "^2.6.1",
    "vue-class-component": "^7.0.0",
    "vue-property-decorator": "^8.2.2",
    "vue-tsx-support": "^3.0.0-dev.3"
  }
}
