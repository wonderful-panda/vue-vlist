{
  "name": "vue-vlist",
  "description": "Light weight vertical list component for Vue",
  "author": "Iwata Hidetaka <iwata0303@gmail.com>",
  "scripts": {
    "tsc": "tsc",
    "typings": "typings install",
    "watchify": "watchify -vd -p browserify-hmr -e example/main.js -o example/dist/build.js",
    "serve": "http-server -c 1 -a localhost",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify --no-bundle-external src/main.ts > dist/build.js"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "vue": "^1.0.26",
    "vueit": "^0.0.2",
    "vue-resizesensor": "^0.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-typescript": "^2.13.6",
    "http-server": "^0.9.0",
    "jade": "^1.11.0",
    "npm-run-all": "^1.6.0",
    "stringify": "^5.1.0",
    "tsify": "^0.16.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "tsify"
    ]
  }
}
