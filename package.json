        "vue-strict-prop": "^0.1.0",
        "vue-template-compiler": "^2.5",
        "vue-template-compiler-loader": "^1.0.4",
        "vue-tsx-support": "^1.1.2",
        "vue-typed-component": "^0.10.0",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.4"
    },
